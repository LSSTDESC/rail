   "source": [
    "Let's generate some fake data."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "efc490a9-cd6b-4994-a5d9-9d753116e5ae",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Fake data with same magnitude in each band\n",
    "i = np.ones(50_000)*23.\n",
    "u = np.full_like(i, 23.0, dtype=np.double)\n",
    "g = np.full_like(i, 23.0, dtype=np.double)\n",
    "r = np.full_like(i, 23.0, dtype=np.double)\n",
    "y = np.full_like(i, 23.0, dtype=np.double)\n",
    "z = np.full_like(i, 23.0, dtype=np.double)\n",
    "redshift = np.random.uniform(size=len(i)) * 2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "d1358e18-e58a-4eb9-84fd-07430fcafd6b",
   "metadata": {},
   "outputs": [],
   "source": [
    "mockdict = {}\n",
    "for label, item in zip(['redshift','u', 'g','r','i', 'z','y'], [redshift,u,g,r,i,z,y]):\n",
    "    mockdict[f'{label}'] = item"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "de92f135-efd4-470c-823d-0797ab520d1a",
   "metadata": {},
   "outputs": [],
   "source": [
    "data = pd.DataFrame(mockdict)\n",
    "data.head()"
   ]
  },
